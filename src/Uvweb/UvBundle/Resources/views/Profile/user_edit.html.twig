{% extends "UvwebUvBundle::layout.html.twig" %}

{% block title %}
    Édition du profil - {{ parent() }}
{% endblock %}

{% block body %}

    <div class="page-header">
        <h2>Édition du profil</h2>
    </div>

    <div class="row-fluid">
        <div class="well">
                {{ form_errors(edit_user_form) }}
            <form method="post" {{ form_enctype(edit_user_form) }}>
                {{ form_widget(edit_user_form) }}
                <input type="submit" class="btn btn-primary" />
            </form>
        </div>
    </div>

{% endblock %}